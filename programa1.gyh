#Programa nao tem erro sintático

:DEC
parametro:INT
fatorial:REAL

:PROG 
# Calcula o fatorial de um número inteiro
LER parametro
fatorial := parametro
ENQTO parametro > 1
 INI
  fatorial := fatorial * (parametro - 1)
  parametro := parametro - 1
 FIM
IMPRIMIR fatorial
IMPRIMIR "Preto Henrique"
